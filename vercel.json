{
  "version": 2,
  "name": "intelligent-creative-canvas",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_APP_NAME": "Intelligent Creative Canvas"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}